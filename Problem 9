#include<stdio.h>
int main()
{
    unsigned long int a,b,c;
    for(a=1000;a>0;a--)
    {
        for(b=1;b<1000;b++)
        {
            c=1000-a-b;
            if(a*a+b*b==c*c)
            {
                printf("a=%lu b=%lu \n",a,b);
                printf("%lu",a*b*c);
                return 0;
            }
        }
    }

}

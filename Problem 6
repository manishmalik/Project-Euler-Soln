#include<stdio.h>
int main()
{
    long long int squaresum=0,sumsquare=0;
    int i,t=1,temp=0;
    for(i=1;i<=100;i++)
    {
        t=i*i;
        squaresum+=t;
    }
    for(i=1;i<=100;i++)
     {
         temp+=i;
     }
    sumsquare=temp*temp;
    printf("%lld",sumsquare-squaresum);
    return 0;

}
